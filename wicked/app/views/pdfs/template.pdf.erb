<!doctype html>
<html>
  <head>
  </head>
  <body>
  <h1>Posts</h1>
  <table>
    <th>Id</th>
    <th>Nombre</th>
    <th>Content</th>
    <% @posts.each do |post| %>
      <tr>
          <td><%= post.id %></td>
          <td><%= post.name %></td>
          <td><%= post.content %></td>
      </tr>
    <% end %>
  </table>
  </body>
</html>